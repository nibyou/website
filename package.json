{
  "name": "website",
  "description": "Nibyou Website build script",
  "scripts": {
    "compile-sass": "sass style/main.scss style/main.css",
    "uglify-css": "uglifycss style/main.css > style/main.min.css",
    "uglify-js": "uglifyjs-folder javascript -o javascript -e",
    "build": "npm run compile-sass && npm run uglify-css && npm run uglify-js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "sass": "^1.49.9",
    "uglifycss": "^0.0.29",
    "uglifyjs-folder": "^3.0.0"
  }
}
